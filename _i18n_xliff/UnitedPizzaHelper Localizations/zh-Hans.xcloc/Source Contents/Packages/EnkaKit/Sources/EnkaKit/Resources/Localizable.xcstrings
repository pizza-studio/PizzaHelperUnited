    "settings.display.artifactRating.considerHyperBloomElectroRoles" : {
      "extractionState" : "manual",
      "localizations" : {
        "en" : {
          "stringUnit" : {
            "state" : "translated",
            "value" : "Accommodating electro hyperbloom builds"
          }
        }
      }
    },
    "settings.display.artifactRating.considerHyperBloomElectroRoles.explanation" : {
      "extractionState" : "manual",
      "localizations" : {
        "en" : {
          "stringUnit" : {
            "state" : "translated",
            "value" : "Genshin Only. E.g.: Raiden Shogun with over-900 elemental master (and related 4-piece artifact sets) can team up with Baizhu, Furina, and Nahida to be the strongest full-auto guided remote hyperbloom team. This disobeys the initial character design of Raiden Shogun but is an acceptable and effective choice with significantly-reduced workload of building-up the entire team with significantly larger firing range than Shinobu-Yukuaki-based close-combat hyperbloom teams. Similar situations may apply to other electro characters in Genshin Impact."
          }
        }
      }
    },
    "settings.display.artifactRating.considerSwirls" : {
      "extractionState" : "manual",
